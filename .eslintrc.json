{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "react/prop-types": "off",
    "multiline-ternary": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off"
  }
}
